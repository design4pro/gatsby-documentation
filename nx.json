{
  "npmScope": "design4pro",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "projects": {
    "gatsby-starter-docs": {
      "tags": []
    },
    "gatsby-starter-docs-e2e": {
      "tags": [],
