    },
    "gatsby-theme-docs-core": {
      "tags": []
    },
    "gatsby-theme-docs": {
      "tags": []
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
