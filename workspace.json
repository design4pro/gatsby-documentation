{
  "version": 1,
  "projects": {
    "gatsby-starter-docs": {
      "root": "apps/gatsby-starter-docs",
      "sourceRoot": "apps/gatsby-starter-docs/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/gatsby-starter-docs/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "gatsby-starter-docs:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "gatsby-starter-docs:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/gatsby-starter-docs/**/*.js",
              "apps/gatsby-starter-docs/**/*.jsx",
              "apps/gatsby-starter-docs/**/*.ts",
              "apps/gatsby-starter-docs/**/*.tsx",
              "apps/gatsby-starter-docs/**/*.spec.ts",
              "apps/gatsby-starter-docs/**/*.spec.tsx",
              "apps/gatsby-starter-docs/**/*.spec.js",
              "apps/gatsby-starter-docs/**/*.spec.jsx",
              "apps/gatsby-starter-docs/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/gatsby-starter-docs/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/gatsby-starter-docs"]
        }
      },
      "tags": []
    },
    "gatsby-starter-docs-e2e": {
      "root": "apps/gatsby-starter-docs-e2e",
      "sourceRoot": "apps/gatsby-starter-docs-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/gatsby-starter-docs-e2e/cypress.json",
            "tsConfig": "apps/gatsby-starter-docs-e2e/tsconfig.e2e.json",
            "devServerTarget": "gatsby-starter-docs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gatsby-starter-docs:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/gatsby-starter-docs-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/gatsby-starter-docs-e2e/**/*"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["gatsby-starter-docs"]
    },
    "gatsby-theme-docs": {
      "root": "libs/gatsby-theme-docs",
      "sourceRoot": "libs/gatsby-theme-docs/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/gatsby-theme-docs/**/*.ts",
              "libs/gatsby-theme-docs/**/*.spec.ts",
              "libs/gatsby-theme-docs/**/*.spec.tsx",
              "libs/gatsby-theme-docs/**/*.spec.js",
              "libs/gatsby-theme-docs/**/*.spec.jsx",
              "libs/gatsby-theme-docs/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "build": {
          "builder": "@nrwl/web:package",
          "options": {
            "outputPath": "dist/libs/gatsby-theme-docs",
            "tsConfig": "libs/gatsby-theme-docs/tsconfig.lib.json",
            "project": "libs/gatsby-theme-docs/package.json",
            "entryFile": "libs/gatsby-theme-docs/src/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ],
            "buildableProjectDepsInPackageJsonType": "dependencies"
          },
          "outputs": ["{options.outputPath}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/gatsby-theme-docs/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/gatsby-theme-docs"]
        }
      },
      "tags": []
    },
    "gatsby-theme-docs-core": {
      "root": "libs/gatsby-theme-docs-core",
      "sourceRoot": "libs/gatsby-theme-docs-core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/gatsby-theme-docs-core/**/*.ts",
              "libs/gatsby-theme-docs-core/**/*.spec.ts",
              "libs/gatsby-theme-docs-core/**/*.spec.tsx",
              "libs/gatsby-theme-docs-core/**/*.spec.js",
              "libs/gatsby-theme-docs-core/**/*.spec.jsx",
              "libs/gatsby-theme-docs-core/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "build": {
          "builder": "@nrwl/web:package",
          "options": {
            "outputPath": "dist/libs/gatsby-theme-docs-core",
            "tsConfig": "libs/gatsby-theme-docs-core/tsconfig.lib.json",
            "project": "libs/gatsby-theme-docs-core/package.json",
            "entryFile": "libs/gatsby-theme-docs-core/src/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ],
            "buildableProjectDepsInPackageJsonType": "dependencies"
          },
          "outputs": ["{options.outputPath}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/gatsby-theme-docs-core/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/gatsby-theme-docs-core"]
        }
      },
      "tags": []
    }
  }
}
