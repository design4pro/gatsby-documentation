{
  "version": 1,
  "projects": {
    "gatsby-starter-docs": {
      "root": "apps/gatsby-starter-docs",
      "sourceRoot": "apps/gatsby-starter-docs/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/gatsby-starter-docs/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "gatsby-starter-docs:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "gatsby-starter-docs:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/gatsby-starter-docs/tsconfig.json",
              "apps/gatsby-starter-docs/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/gatsby-starter-docs/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/gatsby-starter-docs/jest.config.js",
            "tsConfig": "apps/gatsby-starter-docs/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "gatsby-starter-docs-e2e": {
      "root": "apps/gatsby-starter-docs-e2e",
      "sourceRoot": "apps/gatsby-starter-docs-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/gatsby-starter-docs-e2e/cypress.json",
            "tsConfig": "apps/gatsby-starter-docs-e2e/tsconfig.e2e.json",
            "devServerTarget": "gatsby-starter-docs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gatsby-starter-docs:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/gatsby-starter-docs-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/gatsby-starter-docs-e2e/**/*"
            ]
          }
        }
      }
    },
    "gatsby-theme-docs-core": {
      "root": "libs/gatsby-theme-docs-core",
      "sourceRoot": "libs/gatsby-theme-docs-core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/gatsby-theme-docs-core/tsconfig.lib.json",
              "libs/gatsby-theme-docs-core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/gatsby-theme-docs-core/**/*"
            ]
          }
        },
        "build": {
          "builder": "@nrwl/web:package",
          "options": {
            "outputPath": "dist/libs/gatsby-theme-docs-core",
            "tsConfig": "libs/gatsby-theme-docs-core/tsconfig.lib.json",
            "project": "libs/gatsby-theme-docs-core/package.json",
            "entryFile": "libs/gatsby-theme-docs-core/src/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/gatsby-theme-docs-core/jest.config.js",
            "tsConfig": "libs/gatsby-theme-docs-core/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "gatsby-theme-docs": {
      "root": "libs/gatsby-theme-docs",
      "sourceRoot": "libs/gatsby-theme-docs/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/gatsby-theme-docs/tsconfig.lib.json",
              "libs/gatsby-theme-docs/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/gatsby-theme-docs/**/*"]
          }
        },
        "build": {
          "builder": "@nrwl/web:package",
          "options": {
            "outputPath": "dist/libs/gatsby-theme-docs",
            "tsConfig": "libs/gatsby-theme-docs/tsconfig.lib.json",
            "project": "libs/gatsby-theme-docs/package.json",
            "entryFile": "libs/gatsby-theme-docs/src/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/gatsby-theme-docs/jest.config.js",
            "tsConfig": "libs/gatsby-theme-docs/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "schematics": {
    "@nrwl/gatsby": {
      "application": {}
    }
  },
  "defaultProject": "gatsby-starter-docs"
}
